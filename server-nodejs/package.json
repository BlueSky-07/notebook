{
  "name": "notebook-server-nodejs",
  "version": "1.0.0",
  "main": "dist/main.js",
  "types": "dist/main.d.js",
  "scripts": {
    "compose:mysql": "docker compose --profile mysql up --watch",
    "compose:sqlite": "mkdir -p database && touch database/notebook.db && docker compose --profile sqlite up --watch",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/jest/bin/jest --runInBand",
    "test:e2e": "jest --config ../test/jest-e2e.json"
  },
  "author": "BlueSky-07",
  "license": "MIT",
  "description": "",
  "homepage": "https://github.com/BlueSky-07/notebook/tree/master/server-nodejs",
  "dependencies": {
    "@ai-sdk/openai": "^1.3.21",
    "@aws-sdk/client-s3": "^3.800.0",
    "@nestjs/common": "^11.1.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.0",
    "@nestjs/platform-express": "^11.1.0",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/swagger": "^11.1.6",
    "@nestjs/typeorm": "^11.0.0",
    "ai": "^4.3.13",
    "better-sqlite3": "^11.9.1",
    "cheerio": "^1.0.0",
    "inngest": "^3.35.1",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "marked": "^15.0.11",
    "mime-types": "^3.0.1",
    "mysql2": "^3.14.1",
    "nestjs-s3": "^2.1.0-next.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "tencentcloud-sdk-nodejs-hunyuan": "^4.1.23",
    "typeorm": "^0.3.22",
    "undici": "^7.8.0",
    "uuid": "^11.1.0",
    "yaml": "^2.7.1",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.0",
    "@openapitools/openapi-generator-cli": "^2.20.0",
    "@swc/cli": "^0.7.3",
    "@swc/core": "^1.11.24",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.16",
    "@types/mime-types": "^2.1.4",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.15.3",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.25.1",
    "jest": "^29.7.0",
    "nest-openapi-tools": "^6.0.5",
    "prettier": "^3.5.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.0",
    "swagger-ui-express": "^5.0.1",
    "ts-jest": "^29.3.2",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "packageManager": "pnpm@10.10.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3"
    ],
    "ignoredBuiltDependencies": [
      "@nestjs/core",
      "@openapitools/openapi-generator-cli",
      "@scarf/scarf",
      "@swc/core",
      "sqlite3"
    ]
  }
}
