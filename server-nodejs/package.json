{
  "name": "notebook-server-nodejs",
  "version": "1.0.0",
  "main": "dist/main.js",
  "types": "dist/main.d.js",
  "scripts": {
    "compose:mysql": "docker compose --profile mysql up --watch",
    "compose:sqlite": "touch database/notebook.db && docker compose --profile sqlite up --watch",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/jest/bin/jest --runInBand",
    "test:e2e": "jest --config ../test/jest-e2e.json"
  },
  "author": "",
  "license": "MIT",
  "description": "",
  "homepage": "https://github.com/BlueSky-07/notebook/tree/master/server-nodejs",
  "dependencies": {
    "@ai-sdk/openai": "^1.3.6",
    "@nestjs/common": "^11.0.12",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.12",
    "@nestjs/platform-express": "^11.0.12",
    "@nestjs/typeorm": "^11.0.0",
    "ai": "^4.2.10",
    "better-sqlite3": "^11.9.1",
    "inngest": "^3.34.1",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "mysql2": "^3.14.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "typeorm": "^0.3.21",
    "undici": "^7.6.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.5",
    "@nestjs/schematics": "^11.0.2",
    "@nestjs/swagger": "^11.1.0",
    "@nestjs/testing": "^11.0.12",
    "@openapitools/openapi-generator-cli": "^2.18.4",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.11.15",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.13.15",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.23.0",
    "jest": "^29.7.0",
    "nest-openapi-tools": "^6.0.5",
    "prettier": "^3.5.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.0",
    "swagger-ui-express": "^5.0.1",
    "ts-jest": "^29.3.1",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2",
    "yaml": "^2.7.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "packageManager": "pnpm@10.7.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3"
    ],
    "ignoredBuiltDependencies": [
      "@nestjs/core",
      "@openapitools/openapi-generator-cli",
      "@scarf/scarf",
      "@swc/core",
      "sqlite3"
    ]
  }
}
